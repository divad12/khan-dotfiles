
################################################################################
# Khan Academy specific

# Add Phabricator bins to PATH
export PATH="$HOME/khan/devtools/arcanist/khan-bin:$HOME/khan/rbtools/bin:$PATH"

# Activate Python2.7 virtualenv
source ~/.virtualenv/khan27/bin/activate

# Enable autocompletion for git and hg
source ~/khan-dotfiles/git-completion.bash
source ~/khan-dotfiles/hg-completion.bash
